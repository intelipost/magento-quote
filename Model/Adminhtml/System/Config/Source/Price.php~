<?php

class Intelipost_Quote_Model_Adminhtml_System_Config_Source_Price
{

public function toOptionArray()
{
    return array(
        array ('value' => 'product', 'label' => Mage::helper('adminhtml')->__('Yes')),
        array ('value' => 0, 'label'=>Mage::helper('adminhtml')->__('No')),
    );
}

}

